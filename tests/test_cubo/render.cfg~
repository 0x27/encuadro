# Configuracion base
# background_filename: Background image filename
#  default value = bg.png
background_filename=bg.png


# filename: Input model filename
#  default value = model.stl
filename=model.stl


# focal_distance: focal distance
#  default value = 1000
focal_distance=1000


# fov: field of view
#  default value = 13.0412
fov=13.0412


# is_ap: AP/lateral choice
#  default value = true
is_ap=true


# poses_filename: Poses filename
#  default value = poses.txt
poses_filename=poses.txt


# spheres_filename: Spheres filename
#  default value = spheres.txt
spheres_filename=spheres.txt


# use_axes: show world axes
#  default value = false
use_axes=false


# use_background: use background image
#  default value = false
use_background=false


# use_camera: use camera/model motion
#  default value = true
use_camera=true


# use_offscreen: use offscreen rendering
#  default value = true
use_offscreen=true


# use_test_box: show test model
#  default value = true
use_test_box=false


# w_size: window size
#  default value = 480
w_size=512


# w_size_y: window size Y
#  default value = 320
w_size_y=512


